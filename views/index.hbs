<div id="wrapper">

    <div id="page-wrapper" class="gray-bg">
        <div class="row border-bottom">
            <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                <ul class="nav navbar-top-links navbar-right">
                    <li>
                        <span class="welcome-message">VIS - VLS Dashboard</span>
                    </li>

                </ul>

            </nav>
        </div>
        <div class="row wrapper border-bottom white-bg page-heading">
            <div class="col-lg-10">
                <h2>Castle Test Reports</h2>

            </div>
            <div class="col-lg-2">

            </div>
        </div>
        <div class="wrapper wrapper-content animated fadeInRight">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <div class="ibox-tools">
                            </div>
                        </div>
                        <div class="ibox-content">
                            <div class="row">
                                <div class="col-sm-10 m-b-xs">
                                </div>
                                <div class="col-sm-2">
                                    <div class="input-group">
                                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#myModal6">
                                            Add Project
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover dataTables">
                                    <thead>
                                        <tr>
                                            <th></th>
                                            <th>Project Name</th>
                                            <th># of Tests</th>
                                            <th># OK (%)</th>
                                            <th># NOK (%)</th>
                                            <th>STD (%)</th>
                                            <th>Coverage (%)</th>
                                            <th>Update Date</th>
                                            <th>Refresh Date</th>
                                            <th>SW Version</th>
                                            <th>SVN URL</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{#each projects}}
                                        <tr>
                                            <td>
                                                <button type="button" class="btn btn-xs btn-primary sweetalertbutton ld-ext-right " id="{{this.name}}" title="Generate the report">Update</button>
                                                <button type="button" class="btn btn-xs btn-default" title="execute" onclick="runProject('{{this.name}}' ,this)">Execute</button>
                                            </td>
                                            <td>{{ this.proj_name }}</td>
                                            <td>{{ this.num_of_Tests }}</td>
                                            <td>{{ this.num_passed }}</td>
                                            <td>{{ this.num_failed }}</td>
                                            <td>{{ this.num_std }}</td>
                                            <td>{{ this.coverage }}</td>
                                            <td>{{ this.update_data }}</td>
                                            <td>{{ this.refresh_date }}</td>
                                            <td>{{ this.sw_version }}</td>
                                            <td>{{ this.svn_url }}</td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>

            </div>
        </div>
        <div class="footer">
            <div class="pull-right">
                <strong>Copyright</strong> Valeo - VIS &copy; 2017
            </div>
        </div>

    </div>
</div>